{
  "name": "prettstack",
  "version": "1.0.0",
  "description": "Parcel + React + Express + TypeScript + Tailwind",
  "scripts": {
    "client": "parcel watch src/client/index.html",
    "server": "npm run clean && nodemon --watch src/server/**/*.ts src/server/index.ts",
    "clean": "rm -rf dist",
    "travis": "nodemon src/server/index.ts",
    "lint": "eslint src/**/*.{js,ts,tsx} --quiet --fix",
    "reset": "rm -rf node_modules && rm -rf .cache && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurozael/prettstack.git"
  },
  "keywords": [
    "parcel",
    "react",
    "express",
    "typescript",
    "tailwind"
  ],
  "author": "kurozael",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kurozael/prettstack/issues"
  },
  "homepage": "https://github.com/kurozael/prettstack#readme",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/parcel-bundler": "^1.12.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "^9.8.6",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "parcel-bundler": "^1.12.4",
    "postcss": "^7.0.35",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {}
}
